{
  "name": "PersonalSite",
  "version": "1.0.0",
  "description": "Personal site",
  "main": "index.js",
  "engines": {
    "node": "6.2.0"
  },
  "scripts": {
    "precopy": "copyfiles -f ui/data/**/* public/data/posts",
    "postcopy": "copyfiles -f ui/resources/img/**/* public/resources/img",
    "copy": "copyfiles -f ui/resources/fonts/**/* public/resources/fonts",
    "prestart": "npm install --no-optional",
    "start": "babel-node  --presets es2015 ./server.js",
    "start-server": "webpack-dev-server",
    "clean": "rimraf public/*.*",
    "prebuild": "npm run clean",
    "build": "webpack --config ./ui/webpack.prod.config --progress --profile --colors --display-error-details --display-cached",
    "postbuild": "npm run copy"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "body-parser": "^1.0.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.14.0",
    "markdown-react-js": "^0.3.0",
    "newrelic": "^1.28.3",
    "normalizr": "^2.2.1",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "rx-dom": "^7.0.3",
    "rxjs": "^5.0.0-beta.10",
    "rxjs-es": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "author": "",
  "license": "ISC"
}
